//Faça um programa que exiba os números de 1 (inclusive) a 10 (inclusive) em ordem decrescente.

programa
{
	
	inteiro cont
	funcao inicio()
	{
		escreva("Números de 1 a 10 em ordem decrescente")
		para(cont = 10; cont >=1; cont--){
			escreva("\n", cont)
		}
	}

}
